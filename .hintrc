{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "scrollbar-width",
          "scrollbar-color",
          "scrollbar-width: thin",
          "backdrop-filter",
          "user-select"
        ]
      }
    ]
  }
}